{
    "name": "baim/php-login-management",
    "type": "project",
    "authors": [
        {
            "name": "Fitra Fadilana",
            "email": "fitra90@gmail.com"
        }
    ],
    "require": {
        "php" : ">=8"
    },
    "require-dev": {
        "phpunit/phpunit" : "9.5.8"
    },
    "autoload": {
        "psr-4": {
            "Baim\\Belajar\\PHP\\MVC\\" : "app"
        }
    },
    "scripts": {
        "test-all" : "phpunit",
        "test" : "phpunit --configuration phpunit.xml",
        "test-render-view" : "phpunit --configuration phpunit.xml --testsuite \"test render view\"",
        "test-register-user" : "phpunit --configuration phpunit.xml --testsuite \"test register user\"",
        "test-user-service" : "phpunit --configuration phpunit.xml --testsuite \"test register user service\"",
        "test-user-controller" : "phpunit --configuration phpunit.xml --testsuite \"test user controller\"",
        "test-session" : "phpunit --configuration phpunit.xml --testsuite \"test session\"",
        "test-home-controller" : "phpunit --configuration phpunit.xml --testsuite \"test home controller\"",
        "test-middleware" : "phpunit --configuration phpunit.xml --testsuite \"test middleware\"",
        "test-middleware-not-login" : "phpunit --configuration phpunit.xml --testsuite \"test middleware not login\""
    },
    "autoload-dev": {
        "psr-4": {
            "Baim\\Belajar\\PHP\\MVC\\" : "tests"
        }
    }
    
}
